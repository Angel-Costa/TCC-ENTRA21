
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-striped table-hover table-bordered mt-3">
    <thead>
        <tr>
            <td>Código</td>
            <td>Nome</td>            
        </tr>
    </thead>

    <tbody>
        @foreach(Model.Usuario usuario in ViewBag.Usuarios)
        {
            <tr>
                <td>@usuario.Id</td>
                <td>@usuario.Nome</td>                
            </tr>
        }
    </tbody>
</table>